{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true
  },
  "extends": ["airbnb-base"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "linebreak-style": ["windows"],
    "arrow-parens": "off",
    "no-console": "off",
    "no-unused-vars": ["error", { "argsIgnorePattern": "next" }]
  }
}
